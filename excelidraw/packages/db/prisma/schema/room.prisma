model Room {
  id              String    @id @default(cuid())
  name            String
  maxParticipants Int       @default(10)
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  owner           User      @relation("RoomOwner", fields: [ownerId], references: [id])
  ownerId         String
  drawings        Drawing[]
  messages        Message[]
}
