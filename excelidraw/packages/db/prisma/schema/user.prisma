// user.prisma

enum Role {
  USER
  ADMIN
}

model User {
  id         String   @id @default(cuid())
  name       String
  username   String   @unique
  password   String
  email      String   @unique
  role       Role     @default(USER)
  photo      String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  ownedRooms Room[]
  Chat       Chat[]
}
